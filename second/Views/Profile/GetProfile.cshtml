@model second.Domain.Entity.Profile
@{
    Layout = "_Layout";
}


<div class="container-fluid">
    <div class="row">
        
            <h3>Адрес: @Model.Address</h3>
            <br />
            <h3>Возраст: @Model.Age</h3>
        <h3>Логин: @Model.User.Login</h3>
        <a asp-controller="Profile" asp-action="SaveProfile" asp-route-id="@Model.Id" class="btn btn-primary">Изменить</a>
       
           

        
    </div>


    <div>
    </div>
</div>