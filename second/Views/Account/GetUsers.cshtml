@using second.Domain.Entity
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@model IEnumerable<second.Domain.Entity.User>
@{
    Layout = "_Layout";
}

<div class="container-fluid">
    <table class="table">
        <thead>
            <tr>
                <th scope="col">#</th>
                <th scope="col">Имя</th>
                <th scope="col">Роль</th>
               
            </tr>
        </thead>
        <tbody>
            @foreach (var tv in Model)
            {
            <tr>
                <th scope="row">@tv.Id</th>
                    <td> @tv.Login</td>
                    <td>@tv.Role.ToString()</td>
               
            </tr>
            }
           
        </tbody>
    </table>
           
            
        </div>
    </div>
</div>
